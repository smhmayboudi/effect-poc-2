{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    // "noEmit": true,
    "noErrorTruncation": true,
    "outDir": "./build",
    "plugins": [{"name": "@effect/language-service"}],
    "rootDir": "./src",
    "skipLibCheck": true
